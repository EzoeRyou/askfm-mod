{
    "manifest_version": 2,
    "name": "Ask.fm Mod",
    "version": "1.0",


    "description": "ask.fm shortcut mod",

    "permissions": [
        "https://ask.fm/*",
        "http://ask.fm/*"
    ], 

    "content_scripts": [
        {
            "matches": ["https://ask.fm/*", "http://ask.fm/*"],
            "js": ["shortcut.js"]
        }
    ]
    
}
